  "scripts": {
    "start": "react-app-rewired start",
    "start:frontend": "react-app-rewired start",
    "start:backend": "cd server && npm run dev",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "analyze": "node scripts/bundleAnalyzer.js",
    "analyze:static": "node scripts/bundleAnalyzer.js --static",
    "analyze:force": "node scripts/bundleAnalyzer.js --force",
    "analyze:build": "set ANALYZE=true && npm run build",
    "analyze:quick": "npm run analyze:existing",
    "analyze:existing": "npx webpack-bundle-analyzer build/static/js/*.js",
    "analyze:report": "node scripts/bundleReport.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\" \"server/**/*.{js,ts,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\" \"server/**/*.{js,ts,md}\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "imports:convert": "node scripts/convertImports.js",
    "security:audit": "cd server && npm run security:audit",
    "test:apps": "node tests/applicationTest.js",
    "pm2:start": "pm2 start server/ecosystem.config.js",
    "pm2:restart": "pm2 restart all",
    "pm2:stop": "pm2 stop all",
    "pm2:delete": "pm2 delete all",
    "deploy": "npm run build && npm run pm2:restart",
    "convert-csv": "node scripts/csv-to-json.js",
    "lint:console": "grep -r 'console\\.' --include='*.js' --include='*.jsx' --include='*.ts' --include='*.tsx' src/ server/ | head -50 || echo 'No console statements found'",
    "lint:any": "grep -r ': any' --include='*.ts' --include='*.tsx' src/ server/ | head -50 || echo 'No any types found'",
    "quality:check": "npm run lint:console && npm run lint:any && npm run lint && npm run format:check",
    "quality:analyze": "node scripts/quality-check.js",
    "lint:backend": "cd server && npm run lint",
    "type-check": "tsc --noEmit",
    "test:changed": "npm test -- --watchAll=false --passWithNoTests",
    "precommit:all": "npm run format:check && npm run lint && npm run type-check && npm run test:changed && npm audit --audit-level high && npm run security:review && npm run runtime:remind",
    "security:review": "echo 'ðŸ”’ Security review integration configured - manual review required for critical changes'",
    "runtime:remind": "echo 'âš ï¸  REMINDER: If you modified React components or workflow panels, verify runtime functionality with npm start' && echo 'See CLAUDE.md Runtime Testing Requirements for details'",
    "quality:fix": "npm run lint:fix && npm run format",
    "prepare": "husky",
    "prisma:generate": "npx prisma generate --schema prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema prisma/schema.prisma"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/dom": "^10.4.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.1",
    "concurrently": "^8.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "lint-staged": "^16.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "pre-commit": "^1.2.2",
    "prettier": "^3.6.2",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "server/**/*.{js,ts}": [
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|react-syntax-highlighter)/)"
    ],
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs",
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules/",
      "integration\\.test\\.jsx$"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
