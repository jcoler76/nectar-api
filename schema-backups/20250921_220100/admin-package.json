{
  "name": "nectar-admin-backend",
  "version": "1.0.0",
  "description": "Platform Admin Portal Backend API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register src/app.ts\"",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "echo 'Backend linting configured'",
    "db:generate": "prisma generate --schema ../prisma/schema.prisma",
    "db:migrate": "prisma db push --schema ../prisma/schema.prisma",
    "db:studio": "prisma studio --schema ../prisma/schema.prisma",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "admin",
    "platform",
    "nectar"
  ],
  "author": "Nectar Team",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@types/cookie-parser": "^1.4.9",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1",
    "prisma": "^6.15.0",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.1",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
